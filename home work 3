#include <stdio.h>
#include <math.h>


int main() {
	int a, b, c ,max;
	printf("Enter integer number:\n");
	scanf_s("%d", &a);
	printf("Enter integer number:\n");
	scanf_s("%d", &b);
	printf("Enter integer number:\n");
	scanf_s("%d", &c);
	int max_of_two = (a > b) ? a : b;
	max = (max_of_two > c) ? max_of_two : c;
	printf("the biggest number is: max %d", max);


	return 0;
}
